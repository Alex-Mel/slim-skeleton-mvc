{
    "name": "semhoun/slim3-skeleton-mvc",
    "description": "Simple Slim Framework 3 skeleton with Twig, Monolog, Doctrine in Sqlite, JWT authentication.",
    "license": "MIT",
    "type": "project",
    "keywords": ["slim-framework", "skeleton", "mvc", "sqlite", "twig", "monolog", "doctrine", "jwt authentication"],
    "authors": [
        {
            "name": "Nathanael Semhoun",
            "email": "nathanael@semhoun.net",
            "homepage": "http://www.semhoun.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\Controller\\": "src/Controller",
            "App\\Entity\\": "src/Entity",
			"App\\Lib\\": "src/Lib",
			"App\\Command\\" : "src/Command"
        }
    },
    "require": {
        "slim/slim": "^3.1",
        "slim/twig-view": "^2.4",
        "slim/flash": "^0.4.0",
        "monolog/monolog": "^1.24",
        "doctrine/orm": "^2",
        "tuupola/slim-jwt-auth": "^3.1",
        "ramsey/uuid": "^3.8",
        "dflydev/fig-cookies": "^1.0",
		"symfony/console": "^4.2"
    }
}
